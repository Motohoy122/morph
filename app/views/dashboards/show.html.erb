<br />

<div class="booyah-box col-10 offset-1">
  <h1>My Projects</h1>
  <br>
  <% current_user.projects.each do |project| %>
    <h3 class="text-center">
      <%= link_to project.job_name, project_path(project) %>
    </h3>
    <br>
    <div class="row">
      <div class="col-12 text-center">
      <%= link_to "More Info...", project_path(project), class: 'btn btn-lg btn-primary' %>
      </div>
    </div>
    <br>
  <% end %>
</div>