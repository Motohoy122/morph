<br />
<div class="booyah-box col-10 offset-1">
  <h1>Create a Company</h1>
  <br>
  <%= simple_form_for @organization, url: organizations_path do |f| %>
    <%= f.input :company_name %>
    <%= f.input :company_address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip_code %>
    <%= f.input :email %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>